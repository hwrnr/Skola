							<xsd:element name ="datumRodjenja" type="xsd:string"/>
							<xsd:element name ="pol" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="izdavac" maxOccurs="unbounded" minOccurs="0">
