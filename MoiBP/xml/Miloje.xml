						<xsd:sequence>
							<xsd:element name ="registracioniBroj" type="xsd:int"/>
							<xsd:element name ="datumIzdavanja" type="xsd:string"/>
							<xsd:element name ="vaziDo" type="xsd:string"/>
							<xsd:element name ="dokumentIzdaje" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
